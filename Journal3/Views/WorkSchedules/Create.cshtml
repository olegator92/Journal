@model Journal3.Models.WorkSchedule
@{
    ViewBag.Title = "График работы";
}
@Html.Partial("_HeaderPartial")
<h4>Добавить график работы</h4>
<hr />
@using (Html.BeginForm("Create", "WorkSchedules", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.Partial("_EditPartial")
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Добавить" />
        </div>
    </div>
}

